{
  "lib/*.ex": {
    "command": "lib",
    "template": [ "defmodule {camelcase|dot} do", "", "end" ],
    "alternate": "test/{}_test.exs"
  },
  "test/*_test.exs": {
    "command": "test",
    "template": [ "defmodule {camelcase|dot}Test do", "  use ExUnit.Case, async: true", "end" ],
    "alternate": "lib/{}.ex"
  },
  "mix.exs": { "command": "mix"},
  "README.md": { "command": "readme" }
}
